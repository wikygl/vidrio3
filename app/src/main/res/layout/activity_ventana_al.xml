                                    tools:ignore="TooManyViews" />

                                <TextView
                                    android:id="@+id/tvAdaptador"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.72"
                                    android:text="@string/_0_0"
                                    tools:ignore="TooManyViews" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/lyVidrios"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
