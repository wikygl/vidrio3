++                offsetX = getWidth() / 2 - scaledPageWidth / 2;
++            } else {
++                if (offsetX > 0) {
++                    offsetX = 0;
++                } else if (offsetX + scaledPageWidth < getWidth()) {
++                    offsetX = getWidth() - scaledPageWidth;
